    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "tsup": "^8.0.0",
    "vitest": "^3.0.0"
  }
}
